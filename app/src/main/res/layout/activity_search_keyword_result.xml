<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".Search_Snack_Data_from_DB">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="250dp"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/Text_first_key_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:fontFamily="@font/my_font_family"
                android:textSize="15dp">
            </TextView>

            <TextView
                android:id="@+id/Text_second_key_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:fontFamily="@font/my_font_family"
                android:textSize="15dp">
            </TextView>

            <TextView
                android:id="@+id/Text_third_key_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:fontFamily="@font/my_font_family"
                android:textSize="15dp">
            </TextView>
        </LinearLayout>

        <ListView
            android:id="@+id/Search_listView_key"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="#999999"
            android:dividerHeight="1dp">
        </ListView>
    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:id="@+id/button_key_result_back"
        android:fontFamily="@font/my_font_family"
        android:textSize="15dp" />
</LinearLayout>
