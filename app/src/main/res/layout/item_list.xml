<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_margin="5dp"
    android:padding="5dp">

    />

    <TextView
        android:id="@+id/textView_list_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:fontFamily="@font/my_font_family"
        android:textSize="15dp">
    </TextView>

    <TextView
        android:id="@+id/textView_list_taste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:fontFamily="@font/my_font_family"
        android:textSize="15dp">
    </TextView>

    <TextView
        android:id="@+id/textView_list_cost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:fontFamily="@font/my_font_family"
        android:textSize="15dp">
    </TextView>

    <TextView
        android:id="@+id/textView_list_number_of_rate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:fontFamily="@font/my_font_family"
        android:textSize="15dp">
    </TextView>

    <TextView
        android:id="@+id/textView_list_keyword1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:fontFamily="@font/my_font_family"
        android:textSize="15dp">
    </TextView>

    <TextView
        android:id="@+id/textView_list_keyword2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:fontFamily="@font/my_font_family"
        android:textSize="15dp">
    </TextView>

    <TextView
        android:id="@+id/textView_list_keyword3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:fontFamily="@font/my_font_family"
        android:textSize="15dp">
    </TextView>

</LinearLayout>